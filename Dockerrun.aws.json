{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "front",
      "image": "jfourdachon/multi-front",
      "hostname": "front",
      "essential": false
    },
    {
      "name": "api",
      "image": "jfourdachon/multi-api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "jfourdachon/multi-worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "jfourdachon/multi-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "api"]
    }
  ]
}
